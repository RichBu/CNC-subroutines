%Subroutine 23       ,MX---,
(SUB 23)
  ; LSUB ROUTINE BETWEEN CAVITIES
  ; TO CALL THE Y/A FOLLOWED BY TURN BACK ON
  (CALL 22)  ; INCR THE POCKETS
  (IF P145 EQ 0 GOTO N557) ; NOT DOING POCKETS INTERCHANGE Y/A
    ; DOING POCKETS
    ; NEED TO DO ANYTHING ELSE
    ; TURN OFF THE INTERCHANGE
    G29A
    G72A1.00  ; 
    (GOTO N558)
  N557 (CALL 21)
  N558 ; END OF ROUTINE
(RET)
